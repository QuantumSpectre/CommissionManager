<UserControl x:Class="CommissionManager.DashboardPage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:CommissionManager"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">

    <Grid>
        <!-- Main Content -->
        <Grid Background="#272932">
            <!-- Page Content Goes Here -->

            <Label HorizontalAlignment="Center"
                   VerticalAlignment="Center" Background="Transparent"
                   BorderThickness="1" BorderBrush="#37ebf3"
                   Padding="10" FontSize="20" >

                <Button Content="Dashboard Placeholder"
                        Background="Transparent"
                        BorderThickness ="0"
                        Foreground="#37ebf3">
                    
                </Button>
                
            </Label>

        </Grid>

        
    </Grid>
</UserControl>